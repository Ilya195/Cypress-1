{
  "name": "cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:go" : "npx cypress open --config-file desktop.env.config.js",
    "cypress:mobile" : "npx cypress open --config-file mobile.env.config.js",
    "cypress:desk:chrome:on" : "npx cypress run --browser=chrome --headed --config-file desktop.env.config.js",
    "cypress:mobile:chrome:off" : "npx cypress run --browser=chrome --headless --config-file mobile.env.config.js",
    "cypress:desk:electron:off" : "npx cypress run --browser=electron --headless --config-file desktop.env.config.js",
    "cypress:desk:electron:on" : "npx cypress run --browser=electron --headed --config-file desktop.env.config.js"
  },
  
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^10.11.0",
    "jest": "^29.5.0"
  }
}
